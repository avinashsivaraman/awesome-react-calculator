{
  "name": "awesome-react-calculator",
  "version": "1.1.0",
  "description": "awesome-react-calculator",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component && npm run copy-style && npm run copy-style-es",
    "copy-style": "cp -r src/style lib/style",
    "copy-style-es": "cp -r src/style es/style",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "mathjs": "^10.1.0"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "nwb": "0.25.2",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "author": "avinash sivaraman",
  "homepage": "https://github.com/avinashsivaraman/awesome-react-calculator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/avinashsivaraman/awesome-react-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/avinashsivaraman/awesome-react-calculator/issues"
  },
  "keywords": [
    "react-component",
    "calculator",
    "react"
  ]
}
